{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": ["config:base"],
  "regexManagers": [
    {
      "fileMatch": ["^test\.bash$"],
      "matchStrings": [
        "datasource=(?<datasource>.+?) depName=(?<depName>.+?) versioning=(?<versioning>.+?)\\s\\w*version=['\"](?<currentValue>.+)['\"]\\s"
      ],
      "currentDigestTemplate": "d06df0691bf0"
    }
  ]
}
